				},
				"timeout": 60,
				"debug": false
			}
		}
	},
	"private": {
		"bugs": {
			"name": "candidate",
			"es": {
				"host": "http://localhost",
				"port": 9200,
				"index": "private_bugs",
				"type": "bug_version",
				"typed": true,
				"schema": {"$ref": "..../resources/schema/bug_version.json"},
