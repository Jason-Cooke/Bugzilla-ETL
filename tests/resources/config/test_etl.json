	"alias": {
		"increment": 1000000,
		"file": {
			"path": "resources/schema/bugzilla_aliases.json",
			"$ref": "file://~/private.json#alias_file"
		},
		"elasticsearch": {
			"host": "http://localhost",
			"index": "bug_aliases"
		}
	},
	"bugzilla": {
		"filename": "./tests/resources/sql/bugzilla_mini.sql",
		"$ref": "file://~/private.json#bugzilla",
		"host": "localhost",
		"preamble": "from https://github.com/klahnakoski/Bugzilla-ETL",
		"port": 3306,
		"schema": "bugs",
		"debug": true
	},
	"public": {
		"bugs": {
			"host": "http://localhost",
			"port": 9200,
			"index": "public_bugs",
			"type": "bug_version",
			"typed": true,
			"schema": {"$ref":"..../resources/schema/bug_version.json"},
			"timeout": 60,
			"debug": true
		},
		"comments": {
			"host": "http://localhost",
			"port": 9200,
			"index": "public_comments",
			"type": "bug_comment",
			"typed": true,
			"schema": {"$ref":"..../resources/schema/bug_comments.json"},
			"timeout": 60,
			"debug": true
		}
	},
	"private": {
		"bugs": {
			"host": "http://localhost",
			"port": 9200,
			"index": "private_bugs",
			"type": "bug_version",
			"typed": true,
			"schema": {"$ref":"..../resources/schema/bug_version.json"},
			"timeout": 60,
			"debug": true
		},
		"comments": {
			"host": "http://localhost",
			"port": 9200,
			"index": "private_comments",
			"type": "bug_comment",
			"typed": true,
			"schema": {"$ref":"..../resources/schema/bug_comments.json"},
			"timeout": 60,
			"debug": true
		}
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/BMO/ElasticSearch"
		}
	},
	"debug": {
		"trace": true,
		"log": [
			{
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "./results/logs/bz_etl.log",
				"maxBytes": 10000000,
				"backupCount": 200,
				"encoding": "utf8"
			},
			{
				"log_type": "stream",
				"stream": "sys.stdout"
			}
		]
	}
}
